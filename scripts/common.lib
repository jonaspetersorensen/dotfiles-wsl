# Log text separator
read -d '' separator <<- EOF


EOF

printMessage()
{
  text=$1
  echo "$separator"
  echo -e "${White}-----------------------------------------------${Color_Off}"
  echo -e "   $text"
  echo -e "${White}-----------------------------------------------${Color_Off}"
  echo "$separator"
}

printStart()
{
  text=$1  
  printMessage "${Yellow}$text${Color_Off}"
}

printEnd()
{
  text=$1
  printMessage "${Yellow}$text${Color_Off}"
}
